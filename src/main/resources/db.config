CREATE TABLE IF NOT EXISTS inbound_table (
    id INT AUTO_INCREMENT,
    order_no VARCHAR(15) NOT NULL,
    logistic_company VARCHAR(10) NOT NULL,
    tracking_no VARCHAR(25) NOT NULL,
    shipment_date VARCHAR(30),
    shipment_item1 INT,
    shipment_item2 INT,
    shipment_item3 INT,
    receipt_date VARCHAR(30),
    receipt_item1 INT,
    receipt_item2 INT,
    receipt_item3 INT,
    description VARCHAR(50),
    PRIMARY KEY (id)
);